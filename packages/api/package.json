{
  "name": "api",
  "version": "0.0.0",
  "private": "true",
  "type": "module",
  "scripts": {
    "clean": "rimraf out/* .eslintcache",
    "graphql:gen": "graphql-codegen",
    "graphql:w": "graphql-codegen -w",
    "build:http": "vite-node scripts/build.ts src/apollo-http-handler.ts --outfile out/apollo-http-handler/index.cjs",
    "build:websocket": "vite-node scripts/build.ts src/websocket-handler.ts --outfile out/websocket-handler/index.cjs",
    "build:initialize": "vite-node scripts/build.ts src/initialize-handler.ts --outfile out/initialize-handler/index.cjs",
    "build:scheduled": "vite-node scripts/build.ts src/scheduled-handler.ts --outfile out/scheduled-handler/index.cjs",
    "build": "npm run build:http && npm run build:websocket && npm run build:initialize && npm run build:scheduled",
    "test:unit": "vitest run -c ./vitest.config.unit.ts",
    "test:unit:w": "vitest watch -c ./vitest.config.unit.ts",
    "test:int": "vitest run -c ./vitest.config.integration.ts",
    "test:int:w": "vitest watch -c ./vitest.config.integration.ts",
    "test:e2e": "bash -c \"export NODE_ENV=test PORT=4010 DEBUG=*:ERROR,*:INFO; npm run -w dev-server start:detached && npm run test:e2e:run; npm run -w dev-server stop\"",
    "test:e2e:dev": "bash -c \"export NODE_ENV=test PORT=4010; npm run -w dev-server dev\"",
    "test:e2e:run": "vitest run -c ./vitest.config.e2e.ts",
    "test:e2e:w": "vitest watch -c ./vitest.config.e2e.ts",
    "test:bench": "vitest bench -c ./vitest.config.integration.ts",
    "test": "npm run test:unit && npm run test:int && npm run test:e2e",
    "prettify": "prettier --write --ignore-unknown src",
    "eslint": "eslint . --cache --max-warnings 0",
    "type-check": "tsc --noEmit --project tsconfig.tsc.json",
    "lint": "npm run type-check && npm run eslint"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@aws-sdk/client-apigatewaymanagementapi": "^3.451.0",
    "@eddeee888/gcg-typescript-resolver-files": "^0.7.1",
    "@faker-js/faker": "^8.3.1",
    "@graphql-tools/schema": "^10.0.7",
    "@graphql-tools/utils": "^10.5.5",
    "@knemerzitski/superstruct": "^2.2.5",
    "@types/aws-lambda": "^8.10.126",
    "@types/lodash.isequal": "^4.5.8",
    "aws-lambda": "^1.0.7",
    "dataloader": "^2.2.2",
    "dotenv": "^16.3.1",
    "esbuild": "^0.21.4",
    "google-auth-library": "^9.6.3",
    "graphql": "npm:@knemerzitski/graphql@16.9.1",
    "graphql-scalars": "^1.22.4",
    "lodash.isequal": "^4.5.0",
    "map-obj": "^5.0.2",
    "mitt": "npm:@knemerzitski/mitt@^3.0.4",
    "mongodb": "^6.3.0",
    "nanoid": "^5.0.7",
    "source-map-support": "^0.5.21",
    "superstruct": "npm:@knemerzitski/superstruct@^2.2.5",
    "vite-node": "^1.4.0",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^2.1.4",
    "vitest-mock-extended": "^2.0.2",
    "ws": "^8.18.0"
  }
}
