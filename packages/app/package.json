{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "npm run graphql:gen",
    "vite:dev": "vite --host",
    "vite:preview": "vite preview --host",
    "vite:build": "tsc && vite build",
    "graphql:gen": "graphql-codegen",
    "graphql:watch": "graphql-codegen -w",
    "graphql:gen-types": "vite-node scripts/gen-possible-types.ts -f src/__generated__",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest watch",
    "test:comp": "cypress run --component",
    "test": "npm run test:unit && npm run test:comp",
    "cypress": "cypress open",
    "prettify": "prettier --write --ignore-unknown src cypress",
    "eslint": "eslint . --max-warnings 0",
    "type-check": "tsc --noEmit",
    "lint": "npm run type-check && npm run eslint",
    "dev": "npm run vite:dev & npm run graphql:watch",
    "build": "npm run graphql:gen && npm run vite:build"
  },
  "dependencies": {
    "@apollo/client": "npm:@knemerzitski/apollo-client@^3.12.0",
    "@dnd-kit/core": "^6.2.0",
    "@dnd-kit/sortable": "^9.0.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@mui/icons-material": "^5.14.15",
    "@mui/material": "^5.14.15",
    "@tanstack/react-router": "^1.63.2",
    "@tanstack/router-devtools": "^1.63.2",
    "@tanstack/router-plugin": "^1.62.0",
    "@testing-library/react": "^15.0.7",
    "@types/google.accounts": "^0.0.14",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^2.0.0",
    "apollo-link-logger": "^2.0.1",
    "apollo-link-serialize": "npm:@knemerzitski/apollo-link-serialize@^5.1.1",
    "apollo3-cache-persist": "npm:@knemerzitski/apollo3-cache-persist@^0.16.0",
    "cypress": "^13.6.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "graphql": "npm:@knemerzitski/graphql@16.9.1",
    "graphql-ws": "^5.16.0",
    "jsdom": "^24.0.0",
    "map-obj": "^5.0.2",
    "mitt": "npm:@knemerzitski/mitt@^3.0.4",
    "nanoid": "^5.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "superstruct": "npm:@knemerzitski/superstruct@^2.2.5",
    "use-debounce": "^10.0.0",
    "vite": "^4.4.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.1.4",
    "vitest-mock-extended": "^2.0.2",
    "zen-observable-ts": "^1.2.5"
  },
  "peerDependencies": {
    "@parcel/watcher": "^2.3.0"
  }
}
