{
  "name": "api",
  "version": "0.0.0",
  "private": "true",
  "type": "module",
  "scripts": {
    "prepare": "npm run graphql:gen",
    "graphql:gen": "graphql-codegen",
    "graphql:watch": "graphql-codegen -w",
    "test:unit": "vitest run -c ./vitest.config.unit.ts",
    "test:unit:watch": "vitest watch -c ./vitest.config.unit.ts",
    "test:int": "vitest run -c ./vitest.config.integration.ts",
    "test:int:watch": "vitest watch -c ./vitest.config.integration.ts",
    "test:e2e:prepare": "npm run -w dev-server server:build && NODE_ENV=test npm run -w dev-server start-detached && npm run -w dev-server wait:test",
    "test:e2e:run": "vitest run -c ./vitest.config.e2e.ts",
    "test:e2e:cleanup": "npm run -w dev-server stop",
    "test:e2e:watch": "vitest watch -c ./vitest.config.e2e.ts",
    "test:e2e": "npm run test:e2e:prepare && npm run test:e2e:run; npm run test:e2e:cleanup",
    "test": "npm run test:unit && npm run test:int && npm run test:e2e",
    "build:http": "vite-node build.ts src/apollo-http-handler.ts --outfile out/apollo-http-handler/index.js",
    "build:websocket": "vite-node build.ts src/websocket-handler.ts --outfile out/websocket-handler/index.js",
    "build:initialize": "vite-node build.ts src/initialize-handler.ts --outfile out/initialize-handler/index.js",
    "prettify": "prettier --write --ignore-unknown src",
    "eslint": "eslint . --max-warnings 0",
    "type-check": "tsc --noEmit",
    "lint": "npm run type-check && npm run eslint",
    "build": "rm -rf out/* && npm run build:http && npm run build:websocket && npm run build:initialize"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@aws-sdk/client-apigatewaymanagementapi": "^3.451.0",
    "@eddeee888/gcg-typescript-resolver-files": "^0.7.1",
    "@faker-js/faker": "^8.3.1",
    "@graphql-codegen/cli": "^5.0.2",
    "@knemerzitski/superstruct": "^2.2.5",
    "@types/aws-lambda": "^8.10.126",
    "@types/lodash.isequal": "^4.5.8",
    "aws-lambda": "^1.0.7",
    "dataloader": "^2.2.2",
    "dotenv": "^16.3.1",
    "esbuild": "^0.21.4",
    "google-auth-library": "^9.6.3",
    "graphql": "npm:@knemerzitski/graphql@16.9.1",
    "graphql-scalars": "^1.22.4",
    "lodash.isequal": "^4.5.0",
    "map-obj": "^5.0.2",
    "mongodb": "^6.3.0",
    "nanoid": "^5.0.4",
    "source-map-support": "^0.5.21",
    "superstruct": "npm:@knemerzitski/superstruct@^2.2.5",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "~1.4",
    "vitest-mock-extended": "^1.3.1",
    "ws": "^8.18.0",
    "mitt": "npm:@knemerzitski/mitt@^3.0.2"
  }
}
