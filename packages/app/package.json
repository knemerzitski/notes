{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf out/* out-dev/* out-test/* out-dev-pwa/* .eslintcache coverage/* analyze.html app.log tmp/*",
    "graphql:gen:code": "graphql-codegen",
    "graphql:gen:types": "VITE_GRAPHQL_HTTP_URL=http://localhost:4020/graphql vite-node scripts/gen-possible-types.ts -f src/__generated__",
    "graphql:gen": "npm run graphql:gen:code && npm run graphql:gen:types",
    "graphql:w": "graphql-codegen -w",
    "pwa:gen": "pwa-assets-generator",
    "build": "npm run pwa:gen && NODE_ENV=production npm run build:prod && npm run build:test",
    "build:prod": "npm run vite:build",
    "build:test": "NODE_ENV=test npm run vite:build",
    "test:unit": "vitest run",
    "test:unit:w": "vitest watch",
    "test:comp": "cypress run --component",
    "test:e2e": "export NODE_ENV=test PORT=4010 APP_PORT=6173 DEBUG=*:ERROR,*:INFO; npm run -w dev-server start:detached && npm run vite:preview:start:detached && npm run test:e2e:run; OK=$?; npm run -w dev-server stop; npm run vite:preview:stop; exit $OK",
    "test:e2e:run": "cypress run --e2e",
    "test": "npm run test:unit && npm run test:comp && npm run test:e2e",
    "coverage": "vitest run --coverage",
    "vite:build": "vite build",
    "vite:dev": "vite --host",
    "vite:preview": "vite preview --host",
    "vite:preview:start:detached": "vite preview --host --port $APP_PORT > app.log 2>&1 & wait-port $APP_PORT -t 15000",
    "vite:preview:stop": "kill-port $APP_PORT",
    "vite:analyze": "vite-bundle-visualizer --output analyze.html",
    "dev": "npm run graphql:w & npm run vite:dev",
    "cypress": "NODE_ENV=test cypress open",
    "prettify": "prettier --write --ignore-unknown src cypress",
    "eslint": "eslint . --cache --max-warnings 0",
    "eslint:lint-staged": "eslint --fix --max-warnings 0 --no-warn-ignored",
    "type-check": "tsc --noEmit --project tsconfig.tsc.json",
    "lint": "npm run type-check && npm run eslint",
    "gen-demo-gif": "NODE_ENV=test GIF=1 cypress run && mkdir -p tmp && npm run gen-demo-gif:ffmpeg",
    "gen-demo-gif:crop": "ffmpeg -y -i cypress/videos/gen-demo-gif.cy.ts.mp4 -ss 10 -to 20 -vf 'crop=799:601:624:80' tmp/crop.mp4",
    "gen-demo-gif:palette": "ffmpeg -y -i tmp/crop.mp4 -vf 'fps=15,scale=800:-1:flags=lanczos,palettegen' tmp/palette.png",
    "gen-demo-gif:gif": "ffmpeg -y -i tmp/crop.mp4 -i tmp/palette.png -filter_complex 'fps=15,scale=800:-1:flags=lanczos[x];[x][1:v]paletteuse=dither=bayer:bayer_scale=5' docs/demo.gif",
    "gen-demo-gif:ffmpeg": "npm run gen-demo-gif:crop && npm run gen-demo-gif:palette && npm run gen-demo-gif:gif"
  },
  "dependencies": {
    "@apollo/client": "npm:@knemerzitski/apollo-client@^3.12.0",
    "@dnd-kit/core": "^6.2.0",
    "@dnd-kit/sortable": "^9.0.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@faker-js/faker": "^9.6.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@mui/icons-material": "^5.14.15",
    "@mui/material": "^5.14.15",
    "@tanstack/react-router": "^1.63.2",
    "@tanstack/router-devtools": "^1.63.2",
    "@tanstack/router-plugin": "^1.62.0",
    "@testing-library/react": "^15.0.7",
    "@types/google.accounts": "^0.0.14",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/textarea-caret": "^3.0.3",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^2.0.0",
    "apollo-link-logger": "^2.0.1",
    "apollo-link-serialize": "npm:@knemerzitski/apollo-link-serialize@^5.1.1",
    "cypress": "^13.6.3",
    "cypress-vite": "^1.6.0",
    "debug": "^4.4.0",
    "eslint-plugin-cypress": "^4.0.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "fuse.js": "^7.0.0",
    "graphql": "npm:@knemerzitski/graphql@16.9.1",
    "graphql-ws": "^5.16.0",
    "idb-keyval": "^6.2.2",
    "jsdom": "^24.0.0",
    "map-obj": "^5.0.2",
    "mitt": "npm:@knemerzitski/mitt@^4.0.0",
    "nanoid": "^5.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-qr-code": "^2.0.15",
    "superstruct": "npm:@knemerzitski/superstruct@^2.2.5",
    "use-debounce": "^10.0.0",
    "vite": "^5.4.11",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^2.1.4",
    "vitest-mock-extended": "^2.0.2",
    "zen-observable-ts": "^1.2.5"
  },
  "peerDependencies": {
    "@parcel/watcher": "^2.3.0"
  }
}
